{
  "name": "chat-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true npm run test:unit && npm run test:e2e",
    "test:unit": "react-scripts test --env=jsdom --coverage",
    "test:e2e": "webdriver-manager update && protractor"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/services/pwa/registerServiceWorker"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "chromedriver": "^2.35.0",
    "protractor": "^5.3.0",
    "redux-mock-store": "^1.5.1",
    "selenium-standalone": "^6.12.0"
  }
}
